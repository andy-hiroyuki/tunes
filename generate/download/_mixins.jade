mixin meta(desc, keywords)
  meta(name="description" content=desc)
  meta(name="keywords" content=keywords.join(','))

mixin tracktitle(name, time)
  h2
    i.material-icons audiotrack
    span.title= name
    span.time= time

mixin taglist(tags)
  each tag in tags
    button.tag.md-button.md-default-theme(aria-label=tag)
      span= tag
